{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mat-input": {
      "projectType": "library",
      "root": "projects/mat-input",
      "sourceRoot": "projects/mat-input/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mat-input/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mat-input/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mat-input/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mat-input/src/test.ts",
            "tsConfig": "projects/mat-input/tsconfig.spec.json",
            "karmaConfig": "projects/mat-input/karma.conf.js"
          }
        }
      }
    },
    "my-hotels": {
      "projectType": "library",
      "root": "projects/my-hotels",
      "sourceRoot": "projects/my-hotels/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/my-hotels/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/my-hotels/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/my-hotels/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "projects/my-hotels/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/projects/my-hotels"
          ],
          "options": {
            "jestConfig": "projects/my-hotels/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "myFlights": {
      "projectType": "library",
      "root": "projects/my-flights",
      "sourceRoot": "projects/my-flights/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/my-flights/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/my-flights/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/my-flights/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/my-flights/src/test.ts",
            "tsConfig": "projects/my-flights/tsconfig.spec.json",
            "karmaConfig": "projects/my-flights/karma.conf.js"
          }
        }
      }
    },
    "vica-assist": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        },
        "i18n": {
          "sourceLocal": "en-US"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/vica-assist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "200kb",
                  "maximumError": "250kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "vica-assist:build:production"
            },
            "development": {
              "browserTarget": "vica-assist:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vica-assist:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "item": {
      "projectType": "library",
      "root": "projects/item",
      "sourceRoot": "projects/item/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/item/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/item/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/item/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/item/src/test.ts",
            "tsConfig": "projects/item/tsconfig.spec.json",
            "karmaConfig": "projects/item/karma.conf.js"
          }
        }
      }
    }
  }
}
